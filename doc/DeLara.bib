@article{Heymann-DeLara-Chancelier:2022,
  title =	 {Kuhn's equivalence theorem for games in product form},
  journal =	 {Games and Economic Behavior},
  volume =	 135,
  pages =	 {220-240},
  year =	 2022,
  issn =	 {0899-8256},
  doi =		 {https://doi.org/10.1016/j.geb.2022.06.006},
  author =	 {Benjamin Heymann and Michel {De Lara} and Jean-Philippe
                  Chancelier},
  keywords =	 {Games with information, Kuhn's equivalence theorem, Perfect
                  recall, Witsenhausen intrinsic model},
  abstract =	 {We propose an alternative to the tree representation of
                  extensive form games. Games in product form represent
                  information with $\sigma$-fields over a product set, and do
                  not require an explicit description of the play temporal
                  ordering, as opposed to extensive form games on trees. This
                  representation encompasses games with continuum of actions and
                  imperfect information. We adapt and prove Kuhn's theorem ---
                  regarding equivalence between mixed and behavioral strategies
                  under perfect recall --- for games in product form with
                  continuous action sets.}
}

